{
  "name": "@addasoft/skeleton-component-generator",
  "version": "1.1.0",
  "description": "Create skeleton web components using Lit based on given Custom Elements Manifest",
  "main": "index.js",
  "type": "module",
  "module": "index.js",
  "private": false,
  "files": [
    "index.js",
    "tools.js"
  ],
  "scripts": {
    "format": "prettier \"**/*.js\" \"**/*.ts\" --write --ignore-path .gitignore",
    "generate": "node index.js"
  },
  "bin": {
    "skeleton-component-generator": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lucienimmink/skeleton-component-generator.git"
  },
  "keywords": [
    "lit",
    "CEM",
    "custom-elements",
    "api-first"
  ],
  "author": "Lucien Immink",
  "license": "MIT",
  "peerDependencies": {
    "lit": "^3.1.4",
    "tslib": "^2.6.3"
  },
  "volta": {
    "node": "22.11.0",
    "npm": "10.9.0"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "prettier": "^3.3.3"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
